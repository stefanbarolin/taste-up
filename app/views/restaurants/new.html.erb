

<div class="container">
  <h1>Create a new restaurant</h1>
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :adress %>
    <%= f.association :category, collection:["Francais", "Japonais", "Libanais", "Burgers"] %>
    <%= f.submit "Submit restaurant", class: "btn btn-primary" %>
    <%= f.submit %>
  <% end %>
</div>

